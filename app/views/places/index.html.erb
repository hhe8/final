
<div>
  <p> <%= link_to "Add a New Place", "/places/new" %> </p>
  <ul>
    <% @places.each do |place| %>
      <li>
        <p>
          <%= link_to place.title, "/places/#{place.id}" , width: "100" %>  &nbsp;  <%= link_to "Delete", "/places/#{place.id}/delete"%>
        </p>
      </li>
    <% end %>
  </ul>
</div>
